{
    "name": "nikazooz/laravel-captcha",
    "description": "Laravel package to generate and validate CAPTCHA.",
    "keywords": ["laravel", "captcha"],
    "license": "MIT",
    "authors": [
        {
            "name": "Nenad Živanović",
            "email": "nen.zivanovic@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/config": "^8.0",
        "illuminate/contracts": "^8.0",
        "illuminate/http": "^8.0",
        "illuminate/routing": "^8.0",
        "illuminate/session": "^8.0",
        "illuminate/support": "^8.0",
        "illuminate/validation": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "suggest": {
        "ext-gd": "to use GD library based image processing.",
        "ext-imagick": "to use Imagick based image processing."
    },
    "autoload": {
        "psr-4": {
            "Nikazooz\\LaravelCaptcha\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nikazooz\\LaravelCaptcha\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.*-dev"
        },
        "laravel": {
            "providers": [
                "Nikazooz\\LaravelCaptcha\\CaptchaServiceProvider"
            ],
            "aliases": {
                "Captcha": "Nikazooz\\LaravelCaptcha\\Facades\\Captcha"
            }
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
